{
   "action": {
      "default_icon": {
         "48": "images/48.png"
      }
   },
   "background": {
      "service_worker": "background.bundle.js",
      "type": "module"
   },
   "content_scripts": [ {
      "all_frames": false,
      "css": [ "css/yt_summary.css" ],
      "js": [ "contentscript/index.bundle.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
   },
   "description": "Summarize YouTube videos, web articles, and PDFs to save time, powered by ChatGPT, Claude, Mistral AI, and Gemini.",
   "icons": {
      "48": "images/48.png"
   },
   "manifest_version": 3,
   "name": "YouTube Summary with ChatGPT & Claude",
   "options_ui": {
      "open_in_tab": true,
      "page": "options/options.html"
   },
   "permissions": [ "storage" ],
   "version": "1.1.20",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "pdf/build/pdf.worker.min.js" ]
   } ]
}
